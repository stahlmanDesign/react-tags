{
  "name": "react-tag-input-colors",
  "version": "4.3.0",
  "description": "This is a fork of React tags which lets you specify a colour without using classes (to change inline styles and add a calculated colour dynamically)",
  "main": "dist-modules/reactTags.js",
  "scripts": {
    "test": "mocha test/.setup.js test/**/*-test.js",
    "test-watch": "mocha --watch test/.setup.js test/**/*-test.js",
    "dev": "webpack-dev-server --hot --progress --colors --watch --display-error-details --content-base ./",
    "build": "webpack -p && babel lib --out-dir dist-modules"
  },
  "keywords": [
    "react",
    "drag-drop",
    "tags",
    "tag input",
    "react-component",
    "autosuggest"
  ],
  "author": "Justin Stahlman",
  "license": "MIT",
  "repository": "https://github.com/stahlmanDesign/react-tags",
  "dependencies": {
    "lodash.flow": "^3.3.0",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-rc",
    "react-dom": "^0.14.0 || ^15.0.0-rc"
  },
  "devDependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "jsdom": "^9.4.0",
    "lodash": "4.14.1",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.1.0",
    "react-dnd-test-backend": "^1.0.2",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
